import java.util.*;
class Main{
    public static void main(String[] args){
        System.out.println("enter the range ofnumber");
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        for (int i=1;i<=a;i++){
            if(i%2==0){
                System.out.println(i);
            }
        }
    }
}
